labs(x = "Peak area, ion count", y = "Сompound") +
scale_fill_manual(values = c("leaves" = "green", "flowers" = "red")) +
theme_minimal() +
theme(axis.text.x = element_text(angle = 45, hjust = 1, family = "Palatino Linotype"),
