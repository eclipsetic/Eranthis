axis.title = element_text(family = "Palatino Linotype"),
plot.title = element_text(family = "Palatino Linotype"),
legend.text = element_text(family = "Palatino Linotype"),
legend.title = element_text(family = "Palatino Linotype"),
legend.position = "right") +
scale_y_continuous(labels = comma, breaks = seq(0, max(fenolic_long$value), by = 50000000))
